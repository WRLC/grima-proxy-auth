    "type": "project",
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.6",
        "ext-memcached": "*",
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^3.4",
        "laminas/laminas-httphandlerrunner": "^2.10",
        "league/container": "^4.2"
    },
    "autoload": {
